CREATE TABLE `State` (
`StateId` int(11) NOT NULL auto_increment,
`Code` varchar(2) NOT NULL default '',
`Name` varchar(100) NOT NULL default '',
PRIMARY KEY (`StateId`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8;
